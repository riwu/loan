{
  "root": true,
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "overrides": [
    {
      "files": ["**/*.test.js?"],
      "env": {
        "jest": true
      }
    }
  ],
  "rules": {
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "no-console": 0
  }
}
